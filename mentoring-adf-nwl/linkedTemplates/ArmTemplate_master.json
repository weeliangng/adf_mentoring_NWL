{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mentoring-adf-nwl"
		},
		"LS_ABLB_azurementoringstorageaunwl_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_azurementoringstorageaunwl'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mentoring-key-vault-nwl.vault.azure.net/"
		},
		"LS_ABLB_azurementoringdatalakenwl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurementoringdatalakewl.dfs.core.windows.net/"
		},
		"LS_ABLB_azurementoringdatalakwl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurementoringdatalakwl.dfs.core.windows.net/"
		},
		"LS_ABLB_azurementoringstoragenwl_properties_typeProperties_sasUri_secretName": {
			"type": "string",
			"defaultValue": "azurementoringstoragenwl-SAS"
		},
		"LS_ABLB_azurementoringstoregenwl_online_properties_typeProperties_sasUri_secretName": {
			"type": "string",
			"defaultValue": "azurementoringstoragenwl-SAS-online"
		},
		"LS_ASQL_DataMart_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "azure-mentoring-nwl-DartMart"
		},
		"LS_ASQL_movies_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "azure-mentoring-nwl-movies"
		},
		"LS_SQL_CinemaTickets_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "CinemaTickets-MSSQLSERVER"
		},
		"Trigger_iyw_properties_MetadataDrivenCopyTask_iyw_TopLevel_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@{addhours(trigger().scheduledTime, -24)}"
		},
		"Trigger_iyw_properties_MetadataDrivenCopyTask_iyw_TopLevel_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().scheduledTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mentoring-adf-nwl_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ABLB_azurementoringstorageaunwl_connectionString": {
						"value": "[parameters('LS_ABLB_azurementoringstorageaunwl_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_ABLB_azurementoringdatalakenwl_properties_typeProperties_url": {
						"value": "[parameters('LS_ABLB_azurementoringdatalakenwl_properties_typeProperties_url')]"
					},
					"LS_ABLB_azurementoringdatalakwl_properties_typeProperties_url": {
						"value": "[parameters('LS_ABLB_azurementoringdatalakwl_properties_typeProperties_url')]"
					},
					"LS_ABLB_azurementoringstoragenwl_properties_typeProperties_sasUri_secretName": {
						"value": "[parameters('LS_ABLB_azurementoringstoragenwl_properties_typeProperties_sasUri_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mentoring-adf-nwl_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ABLB_azurementoringstoregenwl_online_properties_typeProperties_sasUri_secretName": {
						"value": "[parameters('LS_ABLB_azurementoringstoregenwl_online_properties_typeProperties_sasUri_secretName')]"
					},
					"LS_ASQL_DataMart_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_DataMart_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_ASQL_movies_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_movies_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_SQL_CinemaTickets_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_SQL_CinemaTickets_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-nwl_ArmTemplate_0')]"
			]
		},
		{
			"name": "mentoring-adf-nwl_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-nwl_ArmTemplate_1')]"
			]
		},
		{
			"name": "mentoring-adf-nwl_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Trigger_iyw_properties_MetadataDrivenCopyTask_iyw_TopLevel_parameters_windowStart": {
						"value": "[parameters('Trigger_iyw_properties_MetadataDrivenCopyTask_iyw_TopLevel_parameters_windowStart')]"
					},
					"Trigger_iyw_properties_MetadataDrivenCopyTask_iyw_TopLevel_parameters_windowEnd": {
						"value": "[parameters('Trigger_iyw_properties_MetadataDrivenCopyTask_iyw_TopLevel_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-nwl_ArmTemplate_2')]"
			]
		}
	]
}